{
  "author": "zCloak",
  "bugs": "https://github.com/zCloak-Network/zkid-sdk/issues",
  "contributors": [],
  "description": "zCloak wasm-bridge package",
  "homepage": "https://github.com/zCloak-Network/zkid-sdk/tree/master/utility/wasm-bridge#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@zcloak/wasm-bridge",
  "publishConfig": {
    "directory": "build"
  },
  "repository": {
    "directory": "utility/wasm-bridge",
    "type": "git",
    "url": "https://github.com/zCloak-Network/zkid-sdk.git"
  },
  "sideEffects": false,
  "type": "module",
  "version": "2.0.0",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./package.json": "./package.json",
    "./types": {
      "types": "./types.d.ts",
      "require": "./cjs/types.js",
      "default": "./types.js"
    },
    "./WasmBridge": {
      "types": "./WasmBridge.d.ts",
      "require": "./cjs/WasmBridge.js",
      "default": "./WasmBridge.js"
    },
    "./withWasm": {
      "types": "./withWasm.d.ts",
      "require": "./cjs/withWasm.js",
      "default": "./withWasm.js"
    }
  },
  "dependencies": {
    "@polkadot/util": "^12.1.2"
  }
}
