{
  "author": "zCloak",
  "bugs": "https://github.com/zCloak-Network/zkid-sdk/issues",
  "contributors": [],
  "description": "zCloak cross package",
  "homepage": "https://github.com/zCloak-Network/zkid-sdk/tree/master/utility/cross#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@zcloak/cross",
  "publishConfig": {
    "directory": "build"
  },
  "repository": {
    "directory": "utility/cross",
    "type": "git",
    "url": "https://github.com/zCloak-Network/zkid-sdk.git"
  },
  "sideEffects": false,
  "type": "module",
  "version": "2.0.0",
  "main": "./cjs/node.js",
  "module": "./node.js",
  "browser": "./cjs/browser.js",
  "react-native": "./cjs/react-native.js",
  "types": "./node.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./node.d.ts",
      "react-native": {
        "require": "./cjs/react-native.js",
        "default": "./react-native.js"
      },
      "browser": {
        "require": "./cjs/browser.js",
        "default": "./browser.js"
      },
      "node": {
        "require": "./cjs/node.js",
        "default": "./node.js"
      }
    },
    "./browser": {
      "types": "./browser.d.ts",
      "require": "./cjs/browser.js",
      "default": "./browser.js"
    },
    "./fallback": {
      "types": "./fallback.d.ts",
      "require": "./cjs/fallback.js",
      "default": "./fallback.js"
    },
    "./node": {
      "types": "./node.d.ts",
      "require": "./cjs/node.js",
      "default": "./node.js"
    },
    "./package.json": "./package.json",
    "./react-native": {
      "types": "./react-native.d.ts",
      "require": "./cjs/react-native.js",
      "default": "./react-native.js"
    },
    "./xglobal": {
      "types": "./xglobal.d.ts",
      "require": "./cjs/xglobal.js",
      "default": "./xglobal.js"
    }
  }
}
